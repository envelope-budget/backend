{
  "workbench.colorCustomizations": {
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#00b90f",
    "statusBar.background": "#00860b",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#00b90f",
    "statusBarItem.remoteBackground": "#00860b",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#00860b",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#00860b99",
    "titleBar.inactiveForeground": "#e7e7e799",
    "activityBar.activeBackground": "#00b90f",
    "activityBar.background": "#00b90f",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#e2dfff",
    "activityBarBadge.foreground": "#15202b"
  },
  "peacock.color": "#00860b",
  "editor.formatOnSave": true,
  "[django-html]": {
    "editor.defaultFormatter": "monosans.djlint",
    "editor.quickSuggestions": {
      "other": true,
      "comments": true,
      "strings": true
    }
  },
  "tailwindCSS.emmetCompletions": true,
  "emmet.triggerExpansionOnTab": true,
  "pylint.args": [
    "--load-plugins=pylint_django",
    "--django-settings-module=budgetapp.settings",
  ],
  "djlint.blankLineAfterTag": [
    "endblock",
    "extends"
  ],
  "djlint.blankLineBeforeTag": [
    "block"
  ],
  "cSpell.words": [
    "budgetapp",
    "csrfmiddlewaretoken",
    "csrftoken",
    "datepicker",
    "describedby",
    "Deutsch",
    "endcompress",
    "endfor",
    "evenodd",
    "fitbit",
    "floatformat",
    "flowbite",
    "FOUC",
    "Italiano",
    "labelledby",
    "linecap",
    "linejoin",
    "milliunits",
    "nowrap",
    "optgroup",
    "prefs",
    "PYTHONDONTWRITEBYTECODE",
    "PYTHONUNBUFFERED",
    "sfin",
    "Sidenav",
    "SimpleFIN",
    "sortablejs",
    "stylesheet",
    "subtransactions",
    "tabindex",
    "vendorjs",
    "viewbox",
    "xlink"
  ],
  "djlint.configuration": ".djlintrc",
  "djlint.formatCss": true,
  "djlint.formatJs": true,
  "djlint.ignore": [
    "node_modules/",
    "venv/",
    "staticfiles/",
    "dist/"
  ],
  "djlint.preserveBlankLines": true
}
